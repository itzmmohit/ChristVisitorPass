<div class="ccontainer mt-4">
  <div class="profile-box" style="background: #D32626;">
    {{#each rows}}
    {{#if this.profile_image}}
      <img src="/img_upload/{{this.profile_image}}" class="img-thumbnail rounded-circle mb-2" width="150" height="150">
    {{else}}
      <img src="/img_upload/avatar.png" class="img-thumbnail rounded-circle mb-2" width="150" height="150">
    {{/if}}
    <h3>{{this.name}}</h3>
    <p>{{this.purpose}}</p>
    <div class="profile-bottom">
      <h3>You Are Rejected!</h3>
      <a class="btn btn-primary" href="/visit/nvehicle" role="button">Fill Form Again!</a>
